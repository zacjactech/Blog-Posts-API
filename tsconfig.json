{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "strict": true,
      "outDir": "./dist",
      "rootDir": "./src",
      "esModuleInterop": true, // Add this line
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*"], // Adjust according to your folder structure
    "exclude": ["node_modules"]
  }
  